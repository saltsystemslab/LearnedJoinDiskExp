{
  "name": "uniform_random_64bit keys, 64 bit values",
  "dir": "test_runs/wiki_merge_disk_small",
  "inputs": {
    "common": {
      "key_size": 8,
      "value_size": 8,
      "key_type": "uint64",
      "algo": "create_input",
      "method": "from_sosd",
      "write_result_to_disk": true
    },
    "list": [
      {
        "source": "./datasets/wiki_ts_200M_uint64",
        "name": "10K_1",
        "num_keys": 10000
      },
      {
        "source": "./datasets/wiki_ts_200M_uint64",
        "name": "10K_2",
        "num_keys": 10000
      },
      {
        "source": "./datasets/wiki_ts_200M_uint64",
        "name": "20K",
        "num_keys": 20000
      },
      {
        "source": "./datasets/wiki_ts_200M_uint64",
        "name": "200M",
        "num_keys": 200000000
      }
    ]
  },
  "algos": {
    "repeat": 3,
    "common": {
      "key_size": 8,
      "value_size": 8,
      "key_type": "uint64",
      "load_sstable_in_mem": false,
      "write_result_to_disk": true
    },
    "list": [
      {
        "name": "standard_merge",
        "algo": "standard_merge"
      },
      {
        "name": "learned_merge(PGM_64)",
        "algo": "learned_merge",
        "index": "pgm64"
      },
      {
        "name": "RbTree",
        "algo": "learned_merge",
        "index": "rbtree"
      }
    ]
  },
  "tests": {
    "list": [
      {
        "dir": "10K_to_10K",
        "label": "1:1",
        "label_x": 1,
        "inner_table": "10K_1",
        "outer_table": "10K_2"
      },
      {
        "dir": "10K_to_20K",
        "label": "1:2",
        "label_x": 2,
        "inner_table": "10K_1",
        "outer_table": "20K"
      },
      {
        "dir": "10K_to_200M",
        "label": "1:20000",
        "label_x": 20000,
        "inner_table": "10K_1",
        "outer_table": "200M"
      }
    ]
  },
  "report_format": "csv"
}