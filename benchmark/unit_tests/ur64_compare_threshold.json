        "index": {
          "type": "onelevel_pgm64"
        }
      },
      {
        "name": "learned_merge_threshold_onelevel_pgm",
        "algo": "learned_merge_threshold",
        "threshold": 0,
        "index": {
          "type": "onelevel_pgm64"
        }
      },
      {
        "name": "RbTree_threshold",
        "algo": "learned_merge_threshold",
        "threshold": 0,
        "index": {
          "type": "rbtree"
        }
      }
    ]
  },
  "tests": {
    "list": [
      {
        "dir": "1K_to_100K",
        "label": "1:100",
        "label_x": 100,
        "outer_table": "1K",
        "inner_table": "100K"
      },
      {
