{
  "track_stats": true,
  "inputs": {
    "common": {
      "key_size": 8,
      "value_size": 8,
      "key_type": "str",
      "method": "uniform_dist",
      "algo": "create_input",
      "write_result_to_disk": true
    },
    "list": [
      {
        "name": "1K",
        "num_keys": 1000
      },
      {
        "name": "10K",
        "common_keys_file": "1K",
        "num_keys": 10000
      },
      {
        "name": "100K",
        "common_keys_file": "1K",
        "num_keys": 100000
      }
    ]
  },
  "algos": {
    "repeat": 1,
    "common": {
      "key_size": 8,
      "value_size": 8,
      "key_type": "str",
      "load_sstable_in_mem": false,
      "write_result_to_disk": true
    },
    "list": [
      {
        "name": "standard_merge",
        "algo": "standard_merge"
      },
      {
        "name": "learned_merge_threshold_onelevel_pgm",
        "algo": "learned_merge_threshold",
        "threshold": 5,
        "index": {
          "type": "onelevel_pgm64"
        }
      },
      {
        "name": "learned_merge_threshold_pgm",
        "algo": "learned_merge_threshold",
        "threshold": 5,
        "index": {
          "type": "pgm64"
        }
      },
      {
        "name": "RbTree_threshold",
        "algo": "learned_merge_threshold",
        "threshold": 5,
        "index": {
          "type": "rbtree"
        }
      }
    ]
  },
  "tests": {
    "list": [
      {
        "dir": "1K_to_100K",
        "label": "1:100",
        "label_x": 100,
        "outer_table": "1K",
        "inner_table": "100K"
      },
      {
        "dir": "10K_to_100K",
        "label": "1:10",
        "label_x": 10,
        "outer_table": "10K",
        "inner_table": "100K"
      }
    ]
  },
  "report_format": "csv",
  "metrics": [
        ["result", "duration_sec"],
        ["result", "inner_index_size"],
        ["result", "merge_log", "max_index_error_correction"],
        ["result", "merge_log", "comparison_count"]
  ]
}